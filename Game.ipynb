        "def Computer_choice(Player1_HP,Player2_HP,Player1_Defence, Player2_Defence):\n",
        "  if(Player1_Defence):\n",
        "    return \"attack\"\n",
        "  elif(Player2_Defence):\n",
        "    return \"attack\"\n",
        "  elif(Player1_HP<30):\n",
        "    return \"attack\"\n",
        "  elif(Player2_HP<20):\n",
        "    return \"defence\"\n",
        "  elif random.random()<0.7 :\n",
        "    return \"attack\"\n",
        ""
